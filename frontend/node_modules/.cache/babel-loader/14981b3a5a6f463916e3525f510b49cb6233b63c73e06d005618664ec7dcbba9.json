ath.min(index + chunkSize, books.length);\n\n          // 处理当前分块\n          var chunk = books.slice(start, end).map(function (book) {\n            return _objectSpread(_objectSpread({}, book), {}, {\n              _parsedYear: parseInt(book.year, 10) || 0\n            });\n          });\n          index = end;\n          if (index < books.length) {\n            setTimeout(_processChunk, 0);\n          } else {\n            var sorted = _toConsumableArray(chunk).sort(function (a, b) {\n              if (controller.signal.aborted) {\n                reject(new Error('排序已取消'));\n                return 0;\n              }\n              if (a._parsedYear === b._parsedYear) {\n                return a.title.localeCompare(b.title);\n              }\n              return _this4.sortOrder === 'asc' ? a._parsedYear - b._parsedYear : b._parsedYear - a._parsedYear;\n            });\n            resolve(sorted);\n          }\n        };\n        _processChunk();\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","props","books","type","Array","default","platforms","id","available","total","hasSearched","Boolean","data","sortOrder","debounceTimer","exampleBooks","title","author","year","pages","language","filesize","extension","publisher","source","platform_id","computed","getSortTitle","getSortIcon","sortedBooks","_this","length","result","sortBooks","Promise","then","$nextTick","sortedBooksCache","sortedExampleBooks","_this2","sortedExampleBooksCache","methods","toggleSort","_this3","currentSortAbortController","abort","controller","AbortController","states","currentIndex","indexOf","nextIndex","newSortOrder","resolve","signal","aborted","persistSortOrder","setTimeout","$emit","$forceUpdate","error","console","order","reject","localStorage","setItem","_this4","chunkSize","index","processChunk","Error","start","end","Math","min","chunk","slice","map","book","_objectSpread","_parsedYear","parseInt","sorted","_toConsumableArray","sort","a","b","localeCompare"],"sources":["D:\\zlib\\frontend\\src\\components\\SearchResults.vue"],"sourcesContent":["<template>\r\n  <div class=\"search-results\">\r\n    <!-- 平台状态栏 -->\r\n    <div class=\"platform-status-bar\" v-if=\"platforms.length > 0\">\r\n      <div class=\"platform-status-title\">搜索来源:</div>\r\n      <div class=\"platform-badges\">\r\n        <span v-for=\"platform in platforms\" \r\n              :key=\"platform.id\"\r\n              :class=\"['platform-badge', {'platform-badge-available': platform.available}]\">\r\n          {{ platform.name }}: {{ platform.available ? `找到 ${platform.total} 个结果` : '无结果' }}\r\n        </span>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 搜索结果表格 -->\r\n    <div class=\"results-table\" v-if=\"books.length > 0\">\r\n      <div class=\"table-header\">\r\n        <div class=\"col-book-info\">\r\n          书籍信息\r\n          <button class=\"sort-button\" @click=\"toggleSort\" :title=\"getSortTitle\">\r\n            年份排序\r\n            <i class=\"bi\" :class=\"getSortIcon\"></i>\r\n          </button>\r\n        </div>\r\n        <div class=\"col-author\">作者</div>\r\n        <div class=\"col-publisher\">出版商</div>\r\n        <div class=\"col-link\">直达链接</div>\r\n        <div class=\"col-source\">来源平台</div>\r\n      </div>\r\n\r\n      <div class=\"table-row\" v-for=\"book in sortedBooks\" :key=\"book.isbn\">\r\n        <div class=\"col-book-info\">\r\n          <div class=\"book-title\">{{ book.title }}</div>\r\n          <div class=\"book-details\">\r\n            <span>{{ book.year }}</span>\r\n            <span>| {{ book.language }}</span>\r\n            <span>| {{ book.filesize }}</span>\r\n            <span>| {{ book.extension }}</span>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-author\">{{ book.author }}</div>\r\n        <div class=\"col-publisher\">{{ book.publisher }}</div>\r\n        <div class=\"col-link\">\r\n          <a :href=\"book.book_url\" target=\"_blank\" class=\"download-link\">\r\n            <i class=\"fas fa-download\"></i> 下载\r\n          </a>\r\n        </div>\r\n        <div class=\"col-source\">\r\n          <span :class=\"['source-badge', `source-${book.platform_id}`]\">\r\n            {{ book.source }}\r\n          </span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 无结果提示 -->\r\n    <div v-else-if=\"hasSearched\" class=\"no-results\">\r\n      未找到相关书籍\r\n    </div>\r\n\r\n    <!-- 默认示例书籍 -->\r\n    <div v-else class=\"example-books\">\r\n      <div class=\"example-title\">示例书籍</div>\r\n      <div class=\"results-table\">\r\n        <div class=\"table-header\">\r\n          <div class=\"col-book-info\">\r\n            书籍信息\r\n            <button class=\"sort-button\" @click=\"toggleSort\" :title=\"getSortTitle\">\r\n              年份排序\r\n              <i class=\"bi\" :class=\"getSortIcon\"></i>\r\n            </button>\r\n          </div>\r\n          <div class=\"col-author\">作者</div>\r\n          <div class=\"col-publisher\">出版商</div>\r\n          <div class=\"col-link\">直达链接</div>\r\n          <div class=\"col-source\">来源平台</div>\r\n        </div>\r\n        \r\n        <div class=\"table-row\" v-for=\"book in sortedExampleBooks\" :key=\"book.isbn\">\r\n          <div class=\"col-book-info\">\r\n            <div class=\"book-title\">{{ book.title }}</div>\r\n            <div class=\"book-details\">\r\n              <span>{{ book.year }}</span>\r\n              <span>| {{ book.language }}</span>\r\n              <span>| {{ book.filesize }}</span>\r\n              <span>| {{ book.extension }}</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-author\">{{ book.author }}</div>\r\n          <div class=\"col-publisher\">{{ book.publisher }}</div>\r\n          <div class=\"col-link\">\r\n            <button class=\"download-link\" disabled>\r\n              <i class=\"fas fa-download\"></i> 示例\r\n            </button>\r\n          </div>\r\n          <div class=\"col-source\">\r\n            <span class=\"source-badge\">{{ book.source }}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'SearchResults',\r\n  props: {\r\n    books: {\r\n      type: Array,\r\n      default: () => []\r\n    },\r\n    platforms: {\r\n      type: Array,\r\n      default: () => [\r\n        { id: 'zlibrary', name: 'Z-Library', available: true, total: 0 }\r\n      ]\r\n    },\r\n    hasSearched: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      sortOrder: 'desc',  // 默认降序排列\r\n      debounceTimer: null,\r\n      exampleBooks: [\r\n        {\r\n          title: '示例书籍 1',\r\n          author: '作者名称',\r\n          year: '2024',\r\n          pages: '300',\r\n          language: '简体中文',\r\n          filesize: '10 MB',\r\n          extension: 'PDF',\r\n          publisher: '示例出版社',\r\n          source: 'Z-Library',\r\n          platform_id: 'zlibrary'\r\n        },\r\n        {\r\n          title: '示例书籍 2',\r\n          author: '示例作者',\r\n          year: '2023',\r\n          pages: '250',\r\n          language: '英文',\r\n          filesize: '8 MB',\r\n          extension: 'EPUB',\r\n          publisher: 'Example Press',\r\n          source: 'Z-Library',\r\n          platform_id: 'zlibrary'\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  computed: {\r\n    getSortTitle() {\r\n      switch(this.sortOrder) {\r\n        case 'none':\r\n          return '点击降序排列';\r\n        case 'desc':\r\n          return '点击升序排列';\r\n        case 'asc':\r\n          return '点击取消排序';\r\n        default:\r\n          return '默认排序标题'; // 根据需求调整\r\n      }\r\n    },\r\n    getSortIcon() {\r\n      switch(this.sortOrder) {\r\n        case 'none':\r\n          return 'bi-dash';\r\n        case 'desc':\r\n          return 'bi-sort-down';\r\n        case 'asc':\r\n          return 'bi-sort-up';\r\n        default:\r\n          return 'bi-dash';\r\n      }\r\n    },\r\n    sortedBooks() {\r\n      if (!this.books || this.books.length === 0) {\r\n        return [];\r\n      }\r\n      const result = this.sortBooks(this.books);\r\n      if (result instanceof Promise) {\r\n        result.then(sortedBooks => {\r\n          this.$nextTick(() => {\r\n            this.sortedBooksCache = sortedBooks;\r\n          });\r\n        });\r\n        return this.sortedBooksCache || this.books;\r\n      }\r\n      return result;\r\n    },\r\n    sortedExampleBooks() {\r\n      if (!this.exampleBooks || this.exampleBooks.length === 0) {\r\n        return [];\r\n      }\r\n      const result = this.sortBooks(this.exampleBooks);\r\n      if (result instanceof Promise) {\r\n        result.then(sortedBooks => {\r\n          this.$nextTick(() => {\r\n            this.sortedExampleBooksCache = sortedBooks;\r\n          });\r\n        });\r\n        return this.sortedExampleBooksCache || this.exampleBooks;\r\n      }\r\n      return result;\r\n    },\r\n  },\r\n  methods: {\r\n    toggleSort() {\r\n      // 取消之前的排序操作\r\n      if (this.currentSortAbortController) {\r\n        this.currentSortAbortController.abort();\r\n        this.currentSortAbortController = null;\r\n      }\r\n\r\n      // 创建新的AbortController\r\n      const controller = new AbortController();\r\n      this.currentSortAbortController = controller;\r\n\r\n      // 定义明确的排序状态顺序\r\n      const states = ['none', 'asc', 'desc'];\r\n      const currentIndex = states.indexOf(this.sortOrder);\r\n      const nextIndex = (currentIndex + 1) % states.length;\r\n      const newSortOrder = states[nextIndex];\r\n\r\n      // 使用Promise链处理可取消的排序\r\n      Promise.resolve()\r\n        .then(() => {\r\n          if (controller.signal.aborted) return;\r\n          this.sortOrder = newSortOrder;\r\n          return this.persistSortOrder(newSortOrder);\r\n        })\r\n        .then(() => {\r\n          if (controller.signal.aborted) return;\r\n          return new Promise(resolve => {\r\n            // 添加微任务延迟确保响应式更新\r\n            setTimeout(() => {\r\n              this.$emit('sort-changed', newSortOrder);\r\n              this.$forceUpdate();\r\n              resolve();\r\n            }, 0);\r\n          });\r\n        })\r\n        .catch(error => {\r\n          if (error.name !== 'AbortError') {\r\n            console.error('Sorting error:', error);\r\n            this.sortOrder = 'none';\r\n          }\r\n        })\r\n        .finally(() => {\r\n          this.currentSortAbortController = null;\r\n        });\r\n    },\r\n    persistSortOrder(order) {\r\n      return new Promise((resolve, reject) => {\r\n        try {\r\n          localStorage.setItem('bookSortOrder', order);\r\n          resolve();\r\n        } catch (error) {\r\n          reject(error);\r\n        }\r\n      });\r\n    },\r\n    sortBooks(books) {\r\n      if (this.sortOrder === 'none' || !this.currentSortAbortController) {\r\n        return books;\r\n      }\r\n\r\n      const controller = this.currentSortAbortController;\r\n      \r\n      return new Promise((resolve, reject) => {\r\n        // 分批处理避免阻塞\r\n        const chunkSize = 100;\r\n        let index = 0;\r\n        \r\n        const processChunk = () => {\r\n          if (controller.signal.aborted) {\r\n            reject(new Error('排序已取消'));\r\n            return;\r\n          }\r\n\r\n          const start = index;\r\n          const end = Math.min(index + chunkSize, books.length);\r\n          \r\n          // 处理当前分块\r\n          const chunk = books.slice(start, end).map(book => ({\r\n            ...book,\r\n            _parsedYear: parseInt(book.year, 10) || 0\r\n          }));\r\n\r\n          index = end;\r\n\r\n          if (index < books.length) {\r\n            setTimeout(processChunk, 0);\r\n          } else {\r\n            const sorted = [...chunk].sort((a, b) => {\r\n              if (controller.signal.aborted) {\r\n                reject(new Error('排序已取消'));\r\n                return 0;\r\n              }\r\n              if (a._parsedYear =